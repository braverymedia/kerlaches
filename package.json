{
  "name": "contigoatx",
  "version": "1.0.0",
  "description": "Restaurant website with 11ty and Netlify CMS.",
  "main": "index.js",
  "scripts": {
    "sass": "sass src/_includes/assets/scss:_site/assets/css",
    "postcss": "postcss _site/assets/css/*.css --no-map --replace",
    "build:sass": "npm-run-all --sequential sass postcss",
    "watch:sass": "sass --watch src/_includes/assets/scss:_site/assets/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -c --watch",
    "build:eleventy": "eleventy",
    "watch:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "start": "cross-env NODE_ENV=development npm-run-all --sequential build:js --parallel watch:*",
    "debug:start": "DEBUG=Eleventy* npm start",
    "build": "cross-env npm-run-all build:sass build:js build:eleventy",
    "debug:build": "DEBUG=Eleventy* npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelgoodman/contigoatx.git"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "netlify"
  ],
  "author": "Joel G Goodman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joelgoodman/contigoatx/issues"
  },
  "homepage": "https://github.com/joelgoodman/contigoatx#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "autoprefixer": "^10.3.4",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "html-minifier": "^4.0.0",
    "image-size": "^1.0.0",
    "js-yaml": "^3.14.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^8.3.1",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-container": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.3",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.41.1",
    "terser": "^5.8.0"
  },
  "dependencies": {
    "netlify-cms-app": "^2.15.44"
  }
}
