<meta charset="utf-8">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>{{ title or site.title }}</title>
<meta name="description" content="{{ description or site_description }}">
{% if site.env == 'development' %}
    <link rel="stylesheet" href="/assets/css/contigo.css">
{% else %}
    <link rel="stylesheet" href="/assets/css/contigo.css" defer="defer">
{% endif %}
<meta content="width=device-width, initial-scale=1" name="viewport">
<script src="https://use.typekit.net/nnf5sga.js" type="text/javascript"></script>
<script type="text/javascript">
    try {
        Typekit.load();
    } catch (e) {}
</script>
<script type="text/javascript">
    !function (o, c) {
        var n = c.documentElement,
            t = " w-mod-";
        n.className += t + "js",
        ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
    }(window, document);
</script>
{% if site.env != 'development' and settings.ga %}
<!-- Google Analytics -->
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', '{{ settings.ga }}', 'auto');
    ga('send', 'pageview');
</script>
<!-- End Google Analytics -->
{% endif %}
<link href="images/favicon.ico" rel="shortcut icon" type="image/x-icon">
<link href="images/webclip.png" rel="apple-touch-icon">